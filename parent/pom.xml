		<profile>
			<id>fixcrlf</id>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-antrun-plugin</artifactId>
						<executions>
							<execution>
								<id>fixcrlf</id>
								<phase>generate-sources</phase>
								<configuration>
									<target>
										<fixcrlf srcdir="${basedir}/src" eol="unix" encoding="UTF-8"
